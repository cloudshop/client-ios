    pod 'AFNetworking'
    pod 'MJRefresh'
    pod 'Reachability'
    pod 'MBProgressHUD'
    pod 'MJExtension'
    pod 'DZNEmptyDataSet'
    pod 'SDWebImage', '4.2.3'
    pod 'Masonry', '1.1'
  	
    pod 'UITableView+FDTemplateLayoutCell'
    pod 'WechatOpenSDK'
    pod 'IQKeyboardManager'
    pod 'AMap2DMap' #2D地图SDK (2D和3D不能同时使用)
    pod 'AMapSearch' #搜索功能
    pod 'JPush' #极光
    #  pod 'SwaggerClient', :git => 'https://github.com/GIT_USER_ID/GIT_REPO_ID.git'
end


target 'GongRongPoints' do
    shared_pods
